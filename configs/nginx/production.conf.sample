server
{
    listen 80;
    server_name {SERVER_NAME};
    
    root /var/www/dl/public;

    access_log /var/log/nginx/dl.access.log;
    error_log /var/log/nginx/dl.error.log;
    
    location /
    {
        try_files $uri @bootstrap;
    }
    
    location @bootstrap
    {
        proxy_pass http://127.0.0.1:9001;
    }
}
